{
  "name": "@kakarot-ci/core",
  "version": "0.2.0",
  "description": "Core package for Kakarot CI - AI-powered unit test generation for pull requests",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:clean": "rm -rf dist",
    "build:esm": "node build.js esm",
    "build:cjs": "node build.js cjs",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "npm run build:esm -- --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "semantic-release"
  },
  "keywords": [
    "ci",
    "testing",
    "jest",
    "typescript",
    "ai",
    "llm",
    "pull-request",
    "automation"
  ],
  "author": "David Jansen",
  "license": "BUSL-1.1",
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "typescript": "^5.3.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "semantic-release": "^22.0.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
